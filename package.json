{
  "name": "have-fun-learning-spanish",
  "description": "Web site for Roxana Nunez's publishing of Spanish learning books.",
  "author": "Kevin Smith",
  "keywords": [
    "spanish",
    "spanish learning",
    "book",
    "children",
    "teaching",
    "grade school",
    "elementary school"
  ],
  "homepage": "http://havefunlearningspanish.com/",
  "repository": {
    "type": "git",
    "url": "https://github.com/kevinsmithwebdev/hfls"
  },
  "version": "0.1.1",
  "private": true,
  "dependencies": {
    "@material-ui/core": "^3.9.3",
    "@material-ui/icons": "^3.0.2",
    "@material-ui/styles": "^3.0.0-alpha.10",
    "node-sass-chokidar": "0.0.3",
    "npm-run-all": "^4.0.2",
    "react": "^16.8.0",
    "react-dom": "^16.8.6",
    "react-pdf": "^4.0.5",
    "react-redux": "^5.0.6",
    "react-router": "^4.1.2",
    "react-router-dom": "^4.1.2",
    "react-scripts": "1.0.11",
    "redux": "^3.7.2",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.2.0"
  },
  "scripts": {
    "start-js": "react-scripts start",
    "start": "npm-run-all -p watch-css start-js",
    "starter": "yarn watch-css && yarn start",
    "build": "npm run build-css && react-scripts build",
    "build-css": "node-sass-chokidar src/ -o src/",
    "watch-css": "npm run build-css && node-sass-chokidar src/ -o src/ --watch --recursive",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  }
}
